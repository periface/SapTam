
@{
    ViewBag.Title = "Index";
}
<div id="content">

    <div class="row">
        <div class="col-xs-12 col-sm-7 col-md-7 col-lg-4">
            <h1 class="page-title txt-color-blueDark">
                <i class="fa-fw fa fa-home"></i>
                @L("Projects")
                <span>
                    &gt;
                    @L("ProjectManagement")
                </span>
            </h1>
        </div>
    </div>

    <div class="row">
        <div class="col-xs-12">
            <div class="well well-lg">
                <a data-modal href="@Url.Action("Create")" class="btn btn-lg bg-color-orangeDark txt-color-white"><i class="fa fa-edit"></i> @L("CreateProject")</a>
                @{ Html.RenderAction("Projects"); }
            </div>
        </div>
    </div>
</div>
